all : player player.o ringmaster ringmaster.o helper.o

player : player.o potato.h helper.o helper.h
	gcc -o player player.o potato.h helper.o helper.h

player.o : player.c
	gcc -c player.c


ringmaster : ringmaster.o potato.h helper.o helper.h
	gcc -o ringmaster ringmaster.o potato.h helper.o helper.h

ringmaster.o : ringmaster.c
	gcc -c ringmaster.c

helper.o : helper.c helper.h
	gcc -c helper.c helper.h
